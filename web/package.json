{
  "name": "swordandsnail",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"yarn dev-next\" \"yarn dev-https\"",
    "dev-next": "NODE_TLS_REJECT_UNAUTHORIZED='0' next dev",
    "dev-https": "local-ssl-proxy --key ~/Sites/0_proxy/certs/dev.swordandsnail.com.key --cert ~/Sites/0_proxy/certs/dev.swordandsnail.com.crt --source 4000 --target 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@urql/exchange-auth": "^0.1.6",
    "@urql/exchange-request-policy": "^0.1.5",
    "@urql/exchange-retry": "^0.3.0",
    "bowser": "^2.11.0",
    "graphql": "^16.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^3.0.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "marked": "^4.0.8",
    "next": "12.0.3",
    "next-urql": "^3.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "react-modal": "^3.14.4",
    "sass": "^1.43.4",
    "urql": "^2.0.5"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "local-ssl-proxy": "^1.3.0"
  }
